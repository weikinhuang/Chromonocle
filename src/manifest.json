{
    "name": "Chromonocle",
    "manifest_version": 2,
    "version": "0.0.1",
    "minimum_chrome_version": "20.0",
    "description": "Adds Firebug-like XHR console view details to Chrome",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": [
            "js/chrome/background.js"
        ]
    },
    "icons": {
        "128": "images/icon128.png"
    },
    "permissions": [
        "webRequestBlocking",
        "webRequest",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
